{
  //Obviously this is the sample pulled straight from Google's getting started
  //which can be found: https://developer.chrome.com/extensions/getstarted
  //Look here for more manifest info: https://developer.chrome.com/extensions/manifest
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click this shit!"
  },
  "permissions": [
    "activeTab",  //allows the extension to see the url
    "https://ajax.googleapis.com/" //allows the extension to url google api
  ]
}